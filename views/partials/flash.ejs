<% if(success && success.length) { %>

<div class="alert alert-success alert-dissmisible fade show" role="success">
   <%= success %>
   <button type="button" class="close" data-bs-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
   </button>
</div>

    <% } %>


<% if(error && error.length) { %>

    <div class="alert alert-danger alert-dissmisible fade show" role="danger">
       <%= error %>
       <button type="button" class="close" data-bs-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
       </button>
    </div>
    
        <% } %>